{
  "name": "lol-trophy-hunter-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "meteor run --settings settings.json --port 3000",
    "debug": "meteor run --inspect-brk --settings settings.json --port 3000",
    "production": "meteor run --production --settings settings.json --port 3000",
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "meteor": {
    "mainModule": {
      "modern": "client/index.js",
      "legacy": false,
      "server": "server/index.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "plugins": [
      "import"
    ],
    "settings": {
      "import/resolver": {
        "meteor": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      },
      "import/extensions": [
        ".js",
        ".jsx"
      ]
    },
    "rules": {
      "import/extensions": [
        2,
        {
          "jsx": "never"
        }
      ]
    }
  },
  "dependencies": {
    "@babel/runtime": "7.1.2",
    "@material-ui/core": "3.3.2",
    "@material-ui/icons": "3.0.1",
    "colors": "1.3.2",
    "escape-string-regexp": "1.0.5",
    "fibers": "3.0.0",
    "heatmap.js": "2.0.5",
    "html-to-react": "1.3.3",
    "lodash.defaults": "4.2.0",
    "lodash.flatten": "4.4.0",
    "lodash.get": "4.4.2",
    "lodash.groupby": "4.6.0",
    "lodash.isequal": "4.5.0",
    "lodash.pick": "4.4.0",
    "lodash.throttle": "4.1.1",
    "lodash.zip": "4.2.0",
    "markdown-it": "8.4.2",
    "material-ui": "0.20.2",
    "meteor-deque": "2.1.0",
    "meteor-node-stubs": "0.4.1",
    "meteor-publish-join": "1.1.7",
    "moment": "2.22.2",
    "node-cache": "4.2.0",
    "patreon": "0.4.1",
    "prop-types": "15.6.2",
    "query-string": "6.2.0",
    "rc-slider": "8.6.3",
    "react": "16.6.0",
    "react-countdown-now": "1.3.0",
    "react-dom": "16.6.0",
    "react-ga": "2.5.3",
    "react-infinite-scroller": "1.2.2",
    "react-redux": "5.1.0",
    "react-router-dom": "4.3.1",
    "recharts": "1.3.5",
    "redux": "4.0.1",
    "redux-persist": "5.10.0",
    "redux-thunk": "2.3.0",
    "simpl-schema": "1.5.3"
  },
  "devDependencies": {
    "@storybook/react": "4.0.2",
    "@storybook/ui": "4.0.2",
    "babel-cli": "6.26.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-meteor": "7.1.2",
    "babel-preset-minify": "0.5.0",
    "babel-preset-react": "6.24.1",
    "babelrc-rollup": "3.0.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "eslint": "5.8.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-import-resolver-meteor": "0.4.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.27.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-meteor": "5.1.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "fs": "0.0.2",
    "husky": "1.1.3",
    "jest": "23.6.0",
    "lint-staged": "8.0.4",
    "lodash.defaultsdeep": "4.6.0",
    "recompose": "0.30.0",
    "prettier": "1.14.3",
    "rollup": "0.66.6",
    "rollup-plugin-babel": "4.0.3",
    "yargs": "12.0.2"
  }
}